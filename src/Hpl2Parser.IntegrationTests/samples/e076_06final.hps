		SetEntityActive("llamadolulu", true);
		AddEffectVoice("Taptap.ogg", "", "Avisos", "Luluvoz_final_4", false, "" ,0.0f ,0.0f );
		}	
	}
 
	void llamadolulu(string &in asEntity)
	{
