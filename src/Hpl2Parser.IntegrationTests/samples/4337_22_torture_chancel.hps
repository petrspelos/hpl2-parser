//A little CheckPoint
void CPCallAltar(string &in asName, int alCount)
{
	//START HARD MODE ENDING CHECK
	if(GetHardModeEndingOn())
	{
		StartHardModeEnding("");
		return;
	}
	
	SetPlayerGlobalDeathCount(GetPlayerGlobalDeathCount() + 1);
	//END HARD MODE ENDING CHECK
	ResetDoorIfBroken("sewer_arched_2");
	ResetDoorIfBroken("sewer_arched_3");
	ResetDoorIfBroken("sewer_arched_4");
	ResetDoorIfBroken("sewer_arched_5");
	ResetDoorIfBroken("sewer_arched_6");
	
	StartPlayerLookAt("AreaRitualTable", 10.0f, 10.0f, "");
	
	AddTimer("cp", 0.1f, "TimerCPCall");
	AddTimer("stop", 0.75f, "TimerCPCall");
	if(alCount < 2 && GetEasyModeOn() <= 0)
		AddTimer("enemy", RandFloat(6.0f, 10.0f), "TimerCPCall");
	
	SetLocalVarInt("CPAltar", 1);
	
	PlayMusic("22_amb.ogg", true, 1.0f, 4.0f, 0, true);
	CheckPoint("checkpoint_default", "PlayerStartArea_6", "CPCallAltar", "", "");
}
void TimerCPCall(string &in asTimer)
{
	if(asTimer == "cp"){
		PlayGuiSound("00_cuts.ogg", 1.0f);
		PlayerReact(false, 2);
		return;
	}
	else if(asTimer == "enemy")
	{
		int i = RandInt(0, 2);
		ClearEnemyPatrolNodes(GetStringWithExtraModesSuffix("servant_grunt_5"));
		
		AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_22", 0.25f, "");
		AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_6", 3.0f, "");
		if(i == 0)
			AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_104", GetEnemyIdleTime(5.0f), "");
		else if(i == 1)
			AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_45", GetEnemyIdleTime(5.0f), "");
		else if(i == 2)
			AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_19", GetEnemyIdleTime(5.0f), "");
		
		AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_6", 1.0f, "");
		AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_22", RandFloat(1.1f, 2.9f), "");
		AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_59", RandFloat(1.1f, 2.9f), "");
		AddEnemyPatrolNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_177", RandFloat(1.1f, 2.9f), "");
		
		TeleportEnemyToNode(GetStringWithExtraModesSuffix("servant_grunt_5"), "PathNodeArea_57", true);
		SetEntityActive(GetStringWithExtraModesSuffix("servant_grunt_5"), true);
		
		SetLocalVarInt("NoGrunt", 0);
		
		return;
	}
	else if(asTimer == "stop")
	{
		GiveSanityDamage(10.0f, true);
		StopPlayerLookAt();
	}
}


