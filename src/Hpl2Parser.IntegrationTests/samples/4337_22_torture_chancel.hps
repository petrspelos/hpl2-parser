void CollideExtraChase(string &in asParent, string &in asChild, int alState)
{
	string sEnemyA = GetStringWithExtraModesSuffix("servant_grunt_4");
	string sEnemyB = GetStringWithExtraModesSuffix("servant_grunt_5");
	string sEnemyStateA = GetEnemyStateName(sEnemyA);
	string sEnemyStateB = GetEnemyStateName(sEnemyB);
	
	if(StringContains(asParent, "grunt"))
	{
		SetLocalVarInt("GruntIsInsideChaseArea", alState);
		AddDebugMessage("Grunt inside chase area = " + alState, false);
		return;
	}
	else
	{
		if(GetLocalVarInt("GruntIsInsideChaseArea") >= 0)
		{
			AddDebugMessage("Grunt is inside chase area! Cannot start a new chase!", false);
			return;
		}
		else if(StringContains(sEnemyStateA, "Hunt") || StringContains(sEnemyStateA, "Attack") || StringContains(sEnemyStateA, "BreakDoor") || StringContains(sEnemyStateB, "Hunt") || StringContains(sEnemyStateB, "Attack") || StringContains(sEnemyStateB, "BreakDoor"))
		{
			AddDebugMessage("Grunt is probably already chasing the player! Cannot start a new chase!", false);
			return;
		}
		else
		{
			SetLocalVarInt("NoGrunt", 0);
			AddDebugMessage("Starting a new grunt chase!", false);
			
			RemoveEntityCollideCallback(asParent, asChild);
			SetEntityActive(asChild, false);
			
			ClearEnemyPatrolNodes(sEnemyA);
			
			SetEntityActive(sEnemyA, false);
			SetEntityActive(sEnemyB, false);
			SetEntityActive(sEnemyA, true);
			
			TeleportEnemyToNode(sEnemyA, "PathNodeArea_196", true);
			
			AddEnemyPatrolNode(sEnemyA, "PathNodeArea_33", 1.0f, "");
			AddEnemyPatrolNode(sEnemyA, "PathNodeArea_6", GetEnemyIdleTime(2.0f), "");
			AddEnemyPatrolNode(sEnemyA, "PathNodeArea_196", 1.0f, "");
			
			AlertEnemyOfPlayerPresence(sEnemyA);
			
			PlayGuiSound("door_level_cellar_close", 0.7f);
			
			PlayerReact(true, 0.5f);
		}
	}
}

