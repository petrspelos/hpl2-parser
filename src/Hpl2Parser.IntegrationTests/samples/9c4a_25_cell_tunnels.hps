	SetMapDisplayNameEntry("Cells");
	
	ClearSavedMaps();
	
	////////////////////////////////////
	// INSANITY SETUP
	SetInsanitySetEnabled("Ch03", true); 
	SetInsanitySetEnabled("Ch02", false); 
	
	////////////////////
